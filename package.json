{
  "name": "millenniumdb-browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "pnpm exec prettier . --write",
    "grammar:fetch": "tsx scripts/fetch-grammar.ts",
    "grammar:all": "pnpm run grammar:gql && pnpm run grammar:mql && pnpm run grammar:sparql",
    "grammar:gql": "java -jar src/grammar/antlr-4.13.1-complete.jar -Dlanguage=JavaScript -no-listener src/grammar/gql/GQLParser.g4 src/grammar/gql/GQLLexer.g4",
    "grammar:mql": "java -jar src/grammar/antlr-4.13.1-complete.jar -Dlanguage=JavaScript -no-listener src/grammar/mql/MQL_Parser.g4 src/grammar/mql/MQL_Lexer.g4",
    "grammar:sparql": "java -jar src/grammar/antlr-4.13.1-complete.jar -Dlanguage=JavaScript -no-listener src/grammar/sparql/SparqlQueryParser.g4 src/grammar/sparql/SparqlQueryLexer.g4"
  },
  "dependencies": {
    "@gfazioli/mantine-split-pane": "^2.2.3",
    "@mantine/core": "^8.1.3",
    "@mantine/hooks": "^8.1.3",
    "@mantine/notifications": "^8.1.3",
    "@monaco-editor/react": "^4.7.0",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-router": "^1.125.4",
    "@tanstack/react-router-devtools": "^1.125.4",
    "ag-grid-community": "^34.0.0",
    "ag-grid-react": "^34.0.0",
    "antlr4": "^4.13.2",
    "clsx": "^2.1.1",
    "millenniumdb-driver": "^1.7.10",
    "monaco-editor": "^0.52.2",
    "npm": "^11.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tanstack/router-plugin": "^1.125.5",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss-preset-mantine": "^1.18.0",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}